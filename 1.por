programa {
  funcao inicio() {
    real num1, num2, subtracao
    
    escreva("--- 1. Subtração --- \n")
    escreva("Digite o primeiro número: ")
    leia(num1)
    escreva("Digite o segundo número: ")
    leia(num2)
    
    subtracao = num1 - num2
    escreva("\nA subtração de ", num1, " por ", num2, " é: ", subtracao, "\n")
}
  }
}

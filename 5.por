programa {
funcao inicio() {
    real preco_original, novo_preco
    
    escreva("--- 5. Desconto de 10% --- \n")
    escreva("Digite o preço do produto: R$ ")
    leia(preco_original)
    
    novo_preco = preco_original * 0.90
    escreva("\nO novo preço com 10% de desconto é: R$ ", novo_preco, "\n")
}
    
  }
}

programa {
funcao inicio() {
    real base_maior, base_menor, altura_t, area_trapezio
    
    escreva("--- 9. Área do Trapézio --- \n")
    escreva("Base Maior: ")
    leia(base_maior)
    escreva("Base Menor: ")
    leia(base_menor)
    escreva("Altura: ")
    leia(altura_t)
    
    area_trapezio = ((base_maior + base_menor) * altura_t) / 2
    escreva("\nA área do trapézio é: ", area_trapezio, "\n")
}
    
  }
}
